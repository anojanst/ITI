<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ITI.ITI" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ITI</name>
    <description>
       International training institute
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        ITI
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/logo.png" />
        <icon density="mdpi" src="res/icon/android/logo.png" />
        <icon density="hdpi" src="res/icon/android/logo.png" />
        <icon density="xhdpi" src="res/icon/android/logo.png" />
        <icon density="xxhdpi" src="res/icon/android/logo.png" />
        <icon density="xxxhdpi" src="res/icon/android/logo.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-network-information" spec="^1.3.4" />
    <plugin name="cordova.plugins.diagnostic" spec="^3.8.1" />
    <plugin name="com.cesidiodibenedetto.filechooser" spec="https://github.com/caixiangsap/filechooser.git" />
    <allow-intent href="http://*/*" launch-external="yes" />
    <allow-intent href="https://*/*" launch-external="yes" />
    <allow-intent href="mailto:*" launch-external="yes" />
    <engine name="android" spec="^6.3.0" />
</widget>
